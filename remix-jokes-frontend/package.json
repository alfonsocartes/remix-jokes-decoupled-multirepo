{
  "private": true,
  "version": "0.0.0",
  "name": "remix-jokes",
  "scripts": {
    "build": "remix build",
    "dev": "node -r dotenv/config node_modules/.bin/remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint": "TIMING=1 eslint app/'**/*.{ts,tsx}' --fix",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf .cache && rm -rf build && rm -rf public/build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "dotenv": "^15.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "ui": "*"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "esbuild-register": "^3.3.2",
    "scripts": "*",
    "tsconfig": "*",
    "typescript": "^4.4.0"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
